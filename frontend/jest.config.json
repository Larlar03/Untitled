{
    "collectCoverageFrom": ["**/*.{ts,tsx}"],
    "coveragePathIgnorePatterns": [
        "<rootDir>/src/App.tsx",
        "<rootDir>/src/main.tsx",
        "<rootDir>/src/types/",
        "<rootDir>/src/vite-env.d.ts"
    ],
    "coverageThreshold": {
        "global": {
            "branches": 80,
            "functions": 60,
            "lines": 80,
            "statements": 80
        }
    },
    "setupFilesAfterEnv": ["./src/setupTests.js"]
}
